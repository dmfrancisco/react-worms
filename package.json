{
  "name": "react-worms",
  "version": "0.1.0",
  "private": true,
  "homepage": "http://dmfrancisco.github.io/react-worms",
  "main": "src/Worm.js",
  "devDependencies": {
    "d3": "^4.7.1",
    "gh-pages": "^0.12.0",
    "node-sass": "^4.5.0",
    "npm-run-all": "^4.0.2",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-scripts": "0.9.3"
  },
  "peerDependencies": {
    "d3": "^4.7.1",
    "react": ">=0.14.0",
    "react-dom": ">=0.14.0"
  },
  "scripts": {
    "start": "npm-run-all -p watch-css start-js",
    "start-js": "react-scripts start",
    "build": "npm run build-css && react-scripts build",
    "build-css": "node-sass src/ -o src/",
    "watch-css": "npm run build-css && node-sass src/ -o src/ --watch --recursive",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015",
            "react"
          ]
        }
      ]
    ]
  }
}
